<Application x:Class="UltimateMechanic.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:UltimateMechanic"
             xmlns:converters="clr-namespace:UltimateMechanic.Converters"
             Startup="App_OnStartup">
    <Application.Resources>
        <converters:CountToVisibilityConverter x:Key="CountToVisibilityConverter"/>
        <converters:BytesToSizeConverter x:Key="BytesToSizeConverter"/>
        <converters:BytesToUnitConverter x:Key="BytesToUnitConverter"/>
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
    </Application.Resources>
</Application>
